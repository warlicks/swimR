standard_clean <- function(data, event_data){
	data <- dplyr::mutate(data, gender = data[1, ], standard = data[2, ]) %>%
	dplyr::select(swim_time = starts_with("X"), gender, standard) %>%
	dplyr::slice(3:15) %>%
	dplyr::bind_cols(., event_data)
	
	data <- swimR:::time_convert(data)
	
	return(data)
}